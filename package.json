{
  "name": "glob-sort",
  "version": "1.1.0",
  "scripts": {
    "prepack": "yarn build",
    "build": "tsup-node",
    "start": "tsup-node --watch",
    "lint:check": "yarn biome check src",
    "lint:fix": "yarn lint:check --write",
    "lint": "yarn ts:check && yarn lint:check",
    "test": "vitest",
    "test:unit": "yarn test:coverage",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "ts:check": "tsc --noEmit"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bvandercar-vt/glob-sort"
  },
  "type": "module",
  "source": "src/sortedGlob.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "author": {
    "name": "Blake Vandercar",
    "email": "bvandercar@outlook.com"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.18.8",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/expect": "^3.2.4",
    "mock-fs": "^5.5.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "resolutions": {}
}
